<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
    PUBLIC "-//mybatis.org//DTD MyBatis Generator Configuration 1.0//EN"
        "https://mybatis.org/dtd/mybatis-3-mapper.dtd">


<mapper namespace="Mybatis.Mapper.UserLandingMapper">

    <resultMap id="LoadingMapping1" type="Mybatis.Mybatis_GJL.UserLoading">
        <id column="id" property="name"/>
        <result column="password" property="password"/>
    </resultMap>

    <select id="Loadingpassword" resultMap="LoadingMapping1">
        select * from Loading where id=#{name}
    </select>

    <insert id="insert">
        insert into Loading(id,password)
        values (#{name},#{password})
    </insert>

    <delete id="deleteone">
        delete from Loading where id=#{name}
    </delete>

    <update id="update">
        update Loading
        set
        password=#{password}
        where
        id=#{name}
    </update>

</mapper>